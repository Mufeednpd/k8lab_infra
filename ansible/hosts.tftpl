[control_plane]
%{ for ip in k8worker_ip ~}
k8master ansible_host=${k8master_ip} ansible_user=ubuntu
%{ endfor ~}

[workers]
%{ for index, ip in k8worker_ip ~}
${hostname}${index+1} ansible_host=${ip} ansible_user=ubuntu
%{ endfor ~}

[jenkins]
%{ for ip in k8worker_ip ~}
jenkins ansible_host=${jenkins_ip} ansible_user=ubuntu
%{ endfor ~}

[all:vars]
ansible_python_interpreter=/usr/bin/python3
